// Place your key bindings in this file to override the defaultsauto[]
[
  {
      "key": "cmd+;",
      "command": "git.pullFrom"
  },
  {
      "key": "cmd+'",
      "command": "git.pushTo"
  },
  {
      "key": "ctrl+shift+f",
      "command": "editor.action.formatDocument",
      "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor"
  },
  {
      "key": "shift+alt+f",
      "command": "-editor.action.formatDocument",
      "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor"
  },
  {
      "key": "ctrl+shift+f",
      "command": "editor.action.formatDocument.none",
      "when": "editorTextFocus && !editorHasDocumentFormattingProvider && !editorReadonly"
  },
  {
      "key": "shift+alt+f",
      "command": "-editor.action.formatDocument.none",
      "when": "editorTextFocus && !editorHasDocumentFormattingProvider && !editorReadonly"
  },
  {
      "key": "ctrl+shift+f",
      "command": "notebook.formatCell",
      "when": "editorHasDocumentFormattingProvider && editorTextFocus && inCompositeEditor && notebookEditable && !editorReadonly && activeEditor == 'workbench.editor.notebook'"
  },
  {
      "key": "shift+alt+f",
      "command": "-notebook.formatCell",
      "when": "editorHasDocumentFormattingProvider && editorTextFocus && inCompositeEditor && notebookEditable && !editorReadonly && activeEditor == 'workbench.editor.notebook'"
  },
  {
      "key": "cmd+0",
      "command": "editor.action.insertSnippet",
      "when": "editorTextFocus",
      "args": {
          "snippet": "\n## [v5.${TM_SELECTED_TEXT:${1:0}}.${TM_SELECTED_TEXT:${2:0}}] - $CURRENT_DATE-$CURRENT_MONTH-$CURRENT_YEAR_SHORT\n\n### Fixed\n\n- ${3:content}\n"
      }
  },
  {
      "key": "cmd+1",
      "command": "editor.action.insertSnippet",
      "when": "editorTextFocus",
      "args": {
          "snippet": "## [v5.${TM_SELECTED_TEXT:${1:00}}.0] - $CURRENT_DATE-$CURRENT_MONTH-$CURRENT_YEAR_SHORT \n ### Release \n - ${2:content}"
      }
  }
]